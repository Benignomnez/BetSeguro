# Required API Keys
ODDS_API_KEY=your_odds_api_key_here

# Optional API Keys - Only needed if AI insights are enabled
OPENAI_API_KEY=your_openai_api_key_here

# Base URLs - Don't change unless you're using a different API provider
NEXT_PUBLIC_API_BASE_URL=https://api.the-odds-api.com/v4

# Feature Flags
# Set to 'true' to enable AI-powered insights (requires OPENAI_API_KEY)
# Set to 'false' to use static insights to avoid OpenAI API costs
NEXT_PUBLIC_ENABLE_AI_INSIGHTS=false

# Deployment URL - Set automatically on Vercel, only needed for local development
# VERCEL_URL=localhost:3000
